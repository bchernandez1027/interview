<h1>Pre-Screen Index</h1>

<table>
  <tr>
  	<th>ID</th>
    <th>Title</th>
  </tr>
 
  <% @prescreens.each do |prescreen| %>
    <tr>
		<td><%= prescreen.id %></td>
		<td><%= prescreen.title %></td>
		<td><%= link_to 'Show', prescreen_path(prescreen) %></td>
	    <td><%= link_to 'Edit', edit_prescreen_path(prescreen) %></td>
	    <td><%= link_to 'Destroy', prescreen_path(prescreen),
                    method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>


<%= link_to 'New Pre-screen', new_prescreen_path %>
